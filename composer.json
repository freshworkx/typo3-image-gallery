{
  "name": "bitmotion/bm-image-gallery",
  "description": "Simple gallery using FileCollections.",
  "type": "typo3-cms-extension",
  "homepage": "https://www.bitmotion.de",
  "license": "GPL-2.0-or-later",
  "support": {
    "issues": "https://github.com/bitmotion/typo3-image-gallery/issues",
    "source": "https://github.com/bitmotion/typo3-image-gallery",
    "email": "typo3-ext@bitmotion.de"
  },
  "authors": [
    {
      "name": "Florian Wessels",
      "email": "f.wessels@bitmotion.de",
      "role": "Developer",
      "homepage": "https://www.bitmotion.de"
    }
  ],
  "require": {
    "typo3/cms-backend": "^8.7 || ^9.5 || ^10.2",
    "typo3/cms-core": "^8.7 || ^9.5 || ^10.2",
    "typo3/cms-extbase": "^8.7 || ^9.5 || ^10.2",
    "typo3/cms-frontend": "^8.7 || ^9.5 || ^10.2"
  },
  "suggest": {
    "typo3/cms-fluid-styled-content": "*"
  },
  "autoload": {
    "psr-4": {
      "Bitmotion\\BmImageGallery\\": "Classes"
    }
  },
  "replace": {
    "bitmotion/bm-image-gallery": "self.version",
    "typo3-ter/bm-image-gallery": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "bm_image_gallery"
    }
  }
}
